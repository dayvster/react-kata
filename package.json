{
  "name": "react-kata",
  "version": "1.2.0",
  "description": "A growing collection of modern React hooks for state, effects, events, and more. Clean API, SSR-safe, and TypeScript ready.",
  "author": "dayvster <dayvster.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./useDebounce": {
      "import": "./dist/useDebounce.mjs",
      "require": "./dist/useDebounce.js",
      "types": "./dist/useDebounce.d.ts"
    },
    "./useThrottle": {
      "import": "./dist/useThrottle.mjs",
      "require": "./dist/useThrottle.js",
      "types": "./dist/useThrottle.d.ts"
    },
    "./useInterval": {
      "import": "./dist/useInterval.mjs",
      "require": "./dist/useInterval.js",
      "types": "./dist/useInterval.d.ts"
    },
    "./useTimeout": {
      "import": "./dist/useTimeout.mjs",
      "require": "./dist/useTimeout.js",
      "types": "./dist/useTimeout.d.ts"
    },
    "./useLocalStorage": {
      "import": "./dist/useLocalStorage.mjs",
      "require": "./dist/useLocalStorage.js",
      "types": "./dist/useLocalStorage.d.ts"
    },
    "./useSessionStorage": {
      "import": "./dist/useSessionStorage.mjs",
      "require": "./dist/useSessionStorage.js",
      "types": "./dist/useSessionStorage.d.ts"
    },
    "./usePrevious": {
      "import": "./dist/usePrevious.mjs",
      "require": "./dist/usePrevious.js",
      "types": "./dist/usePrevious.d.ts"
    },
    "./useClickOutside": {
      "import": "./dist/useClickOutside.mjs",
      "require": "./dist/useClickOutside.js",
      "types": "./dist/useClickOutside.d.ts"
    },
    "./useToggle": {
      "import": "./dist/useToggle.mjs",
      "require": "./dist/useToggle.js",
      "types": "./dist/useToggle.d.ts"
    },
    "./useUpdateEffect": {
      "import": "./dist/useUpdateEffect.mjs",
      "require": "./dist/useUpdateEffect.js",
      "types": "./dist/useUpdateEffect.d.ts"
    },
    "./useMediaQuery": {
      "import": "./dist/useMediaQuery.mjs",
      "require": "./dist/useMediaQuery.js",
      "types": "./dist/useMediaQuery.d.ts"
    },
    "./useWindowSize": {
      "import": "./dist/useWindowSize.mjs",
      "require": "./dist/useWindowSize.js",
      "types": "./dist/useWindowSize.d.ts"
    },
    "./useScrollPosition": {
      "import": "./dist/useScrollPosition.mjs",
      "require": "./dist/useScrollPosition.js",
      "types": "./dist/useScrollPosition.d.ts"
    },
    "./useKeyPress": {
      "import": "./dist/useKeyPress.mjs",
      "require": "./dist/useKeyPress.js",
      "types": "./dist/useKeyPress.d.ts"
    },
    "./useOnKeyPressed": {
      "import": "./dist/useOnKeyPressed.mjs",
      "require": "./dist/useOnKeyPressed.js",
      "types": "./dist/useOnKeyPressed.d.ts"
    },
    "./useFocus": {
      "import": "./dist/useFocus.mjs",
      "require": "./dist/useFocus.js",
      "types": "./dist/useFocus.d.ts"
    },
    "./useHovered": {
      "import": "./dist/useHover.mjs",
      "require": "./dist/useHover.js",
      "types": "./dist/useHover.d.ts"
    },
    "./useEventListener": {
      "import": "./dist/useEventListener.mjs",
      "require": "./dist/useEventListener.js",
      "types": "./dist/useEventListener.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/**/*.ts --format cjs,esm --dts --minify --no-sourcemap --clean --target es2018",
    "dev": "tsup src/**/*.ts --watch --format cjs,esm --dts --target es2018",
    "test": "jest --env=jsdom --testMatch '**/tests/**/*.test.tsx'"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "eslint": "^9.36.0",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.3",
    "ts-jest": "^29.4.4",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
